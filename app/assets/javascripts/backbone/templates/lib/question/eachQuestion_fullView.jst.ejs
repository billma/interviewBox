<div class="q_full">
  <div class="previewImg">
      <span><img  src="<%=image%>"  /></span>
      <div class="q_preview">
          <i class="icon-search"></i>
          <span>preview</span>
      </div>
  </div>

  
  <div class="q_info">
      <span class='q_title'><%=title%></span>
      <a class='q_author' href="">by: billMa</a>
      <span class="q_upVote">14<i class="icon-thumbs-up"></i></span>
      <div class="q_time_full timeago" title="<%=created_at%>"><%=created_at%></div>
      <a class='q_link_full' href="/showQuestion/<%=id%>" >
        <i class="icon-external-link"></i>
        show question
      </a>
  </div>

  
  
  
  <div style="clear:both;"></div>
</div>


